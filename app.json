{
  "expo": {
    "name": "image-captioning",
    "slug": "image-captioning",
    "version": "21.0",
    "scheme": "imgcap",
    "owner": "imgcap",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.calpoly.image-captioning",
      "buildNumber": "0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.calpoly.image_captioning",
      "versionCode": 0,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
